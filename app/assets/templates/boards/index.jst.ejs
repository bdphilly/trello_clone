<h1>This be some tasty Boards</h1>

<ul>
	<% boards.each(function(board) { %>
		<li>
			<a href="#/boards/<%= board.escape("id") %>"> 
				<%= board.escape('title') %>
			</a>
				<button class="destroy btn btn-danger" data-id="<%= board.id %>">Obliterate</button>
		</li>
	<% }) %>
</ul>

<a href="#/boards/new">Create a bombtastic board</a>